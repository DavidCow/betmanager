<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bettingManager.gui.Checkbox1Controller">
   <children>
      <Label alignment="CENTER_RIGHT" prefHeight="21.0" prefWidth="157.0" text="Data" textAlignment="RIGHT" />
      <GridPane prefHeight="100.0" prefWidth="157.0">
      
      	<columnConstraints>
      		<ColumnConstraints hgrow="SOMETIMES" maxWidth="128.0" minWidth="10.0" prefWidth="128.0" />
      		<ColumnConstraints hgrow="SOMETIMES" maxWidth="72.0" minWidth="10.0" prefWidth="27.0" />
      	</columnConstraints>
      	<rowConstraints>
      		<RowConstraints maxHeight="45.0" minHeight="10.0" prefHeight="26.0" vgrow="SOMETIMES" />
      		<RowConstraints maxHeight="79.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="79.0" minHeight="10.0" prefHeight="56.0" vgrow="SOMETIMES" />
      	</rowConstraints>
      
      	<children>
      		<CheckBox fx:id="checkboxGroup1historic" mnemonicParsing="false" onAction="#handleCheckboxGroup1" selected="true" GridPane.columnIndex="1" />
      		<CheckBox fx:id="checkboxGroup1real" mnemonicParsing="false" onAction="#handleCheckboxGroup1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      		<Label text="Historic (Backtest)">
      			<font>
      				<Font size="13.0" />
      			</font>
      		</Label>
      		<Label text="Real" GridPane.rowIndex="1">
      			<font>
      				<Font size="13.0" />
      			</font>
      		</Label>
      	</children>
      	<!-- <HBox.margin> <Insets left="10.0" right="10.0" /> </HBox.margin> -->
      </GridPane>
   </children>
</VBox>
