<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox layoutX="428.0" layoutY="10.0" prefHeight="100.0" prefWidth="217.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bettingManager.gui.LiquidityController">
	<children>
		<Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="17.0" prefWidth="219.0" text="Liquidity" />
		<GridPane>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label text="Greater than" />
				<Label text="Less than" GridPane.rowIndex="1" />
				<TextField fx:id="greaterThanTF" onKeyReleased="#handleGreaterThanLiq" GridPane.columnIndex="1" />
				<TextField fx:id="lessThanTF" onKeyReleased="#handleLessThanLiq" GridPane.columnIndex="1" GridPane.rowIndex="1" />
			</children>
		</GridPane>
		<GridPane>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label text="Between" />
				<Label alignment="CENTER" prefHeight="17.0" prefWidth="53.0" text="and" textAlignment="CENTER" GridPane.columnIndex="2" />
				<TextField fx:id="betweenTF" onKeyReleased="#handleBetweenLiq" GridPane.columnIndex="1" />
				<TextField fx:id="andTF" onKeyReleased="#handleAndLiq" GridPane.columnIndex="3" />
			</children>
		</GridPane>
	</children>
</VBox>
